import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		long[] a = new long[2];
		for(int i=0; i<a.length; i++) {
			a[i] = sc.nextLong();
		}
		long c = sc.nextLong();
		long n = sc.nextLong();
		
		long f = (a[0]*n)+(a[1]);
		
		if(f<=c*n && c>=a[0]) {
			System.out.println(1);
		} else {
			System.out.println(0);
		}
		
	}
}
