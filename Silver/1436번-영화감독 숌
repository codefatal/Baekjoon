import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int cnt = 0;
		int asw = 660;
		while(n>cnt) {
			asw++;
			int tmp=asw;
			while(tmp>=666) {
				if(tmp%1000==666) {
					cnt++;
					break;
				} else {
					tmp/=10;
				}
			}
		}
		System.out.println(asw);
	}
}
