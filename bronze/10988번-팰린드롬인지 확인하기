import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		int count = 0;
		for(int i=0; i<s.length()/2; i++) {
			if(s.charAt(i)==s.charAt(s.length()-i-1)) {
				count++;
			}
		}
		if(s.length()/2==count) {			
			System.out.println(1);
		} else {
			System.out.println(0);
		}
	}
}
