import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int n = sc.nextInt();
		String answer = Integer.toString(num, n);
		answer = answer.toUpperCase();
		
		System.out.println(answer);
		
		
		
	}
}
