import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		String[] sArr = new String[t];
		for(int i=0; i<t; i++) {			
			sArr[i] = sc.next();
			System.out.print(sArr[i].charAt(0));
			System.out.print(sArr[i].charAt(sArr[i].length()-1));
			System.out.println();
		}
		
	}
}
