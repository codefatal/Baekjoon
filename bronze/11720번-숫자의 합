import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String str = sc.next();
		int[] arrNum = new int[n];
		int sum = 0;
		for(int i=0; i<str.length(); i++) {
			arrNum[i] = str.charAt(i)-48;
			sum+=arrNum[i];
		}
		System.out.println(sum);
	}
}
